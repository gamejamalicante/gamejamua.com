<div class="item achievement">
    <h3><i class="fa fa-thumbs-up"></i> A <strong><a href="{{ path('gamejam_user_profile', {user: item.user.username}) }}">{{ item.user }}</a></strong> le gusta <strong><a href="{{ path('gamejam_game', {game: item.game.nameSlug}) }}">{{ item.game }}</a></strong></h3>
    <span class="date"><i class="fa fa-clock-o"></i> {{ time_diff(item.date) }}</span>
    <div class="clearfix"></div>
</div>